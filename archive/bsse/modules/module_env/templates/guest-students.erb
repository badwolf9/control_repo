##
## guest-students modulefile / mainly a stub
##
##
proc ModulesHelp { } {
        global version modroot

        puts stderr "\tmodule- loads the environment for guest-students\n"
}

#module-whatis   "provides the guest-students module environment"

<% if operatingsystem =~ /RedHat|CentOS/ %>
set             modroot                 /usr/local/guest-students/el<%= lsbmajdistrelease %>/etc/modulefiles
setenv		MODULES_DISTRO		"el<%= lsbmajdistrelease %>"
<% else %>
set             modroot                 /usr/local/guest-students/other/etc/modulefiles
<% end %>

#append-path     MODULEPATH              $modroot

module load repo/bsse
