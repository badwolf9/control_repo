<% if @hostname =~ /bs-wiki*|bs-jira*|bs-nagios*|bs-svn*|bs-jpk?/ then -%>
#
#	Default BSSE nsswitch.conf without OpenLDAP
#
passwd:     files 
shadow:     files 
group:      files 
<% elsif @hostname =~ /bs-thinlinc01/ -%>
passwd:     files sss
shadow:     files sss
group:      files sss
<% else -%>
#
#	Default BSSE nsswitch.conf for OpenLDAP use
#
passwd:     files ldap
shadow:     files ldap
group:      files ldap
<% end -%>

<% if @operatingsystem =~ /Debian/ then -%>
hosts:          files mdns4_minimal [NOTFOUND=return] dns mdns4
<% else -%>
hosts:      files dns
<% end -%>
<% if @hostname =~ /bs-thinlinc01/ -%>
netgroup:   sss [TRYAGAIN=continue]
<% elsif @hostname !~ /bs-wiki*|bs-jira*|bs-svn*|bs-nagios*|bs-jpk?/ then -%>
netgroup:   ldap [TRYAGAIN=continue]
<% end -%>

ethers:     files
netmasks:   files
networks:   files
protocols:  files
rpc:        files
services:   files

automount:  files
aliases:    files
