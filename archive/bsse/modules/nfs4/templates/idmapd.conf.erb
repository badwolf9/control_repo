[General]

Verbosity = 0
<% if @operatingsystem !~ /Debian|Ubuntu/ and @operatingsystemrelease.to_i < 12 %>
Pipefs-Directory = /var/lib/nfs/rpc_pipefs
<% end %>
Domain = d.ethz.ch

[Mapping]

<% if @operatingsystem =~ /Debian|Ubuntu/ %>
Nobody-User = nobody
Nobody-Group = nogroup
<% else %>
Nobody-User = nfsnobody
Nobody-Group = nfsnobody
<% end %>

[Translation]
Method = nsswitch
