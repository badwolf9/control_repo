#
# Ubuntu doesn't allow sudo without a tty -> exclude on bs-jpk*
<% if @lsbdistid =~ /Ubuntu/ -%># User nrpe needs to run check_md_raid as user root
Defaults:nrpe  !requiretty
    <% elsif classes.include?("service-nfsserver") %>
    <% elsif @hostname =~ /bs-monitor|bs-dsvr20|bs-nagios|bs-hpsvr|bs-jpk|bs-dalc|bs-borgwardt|bs-dsvr79|bs-dsvr82|bs-gene/ -%>
    <% elsif @hostname =~ /bs-stelling03|bs-stelling04|bs-jupyterhub01/ -%># User bs-jhub needs to run jupertyhub as user root
Defaults:bs-jhub  !requiretty
    <% elsif @hostname =~ /bs-openbis|bs-basysbio|bs-cifex/ -%>Defaults!/usr/lib64/nagios/plugins/check_postgres.pl !requiretty
    <% else -%>Defaults    requiretty
Defaults:nrpe  !requiretty
<% end -%>

<% if @hostname =~ /bs-jpk/ %># Defaults    !visiblepw
    <% else %>Defaults    !visiblepw
<% end %>
